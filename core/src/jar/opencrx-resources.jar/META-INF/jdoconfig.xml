        <property name="org.openmdx.jdo.DataManager.restPlugIn[0]" value="org.opencrx.kernel.rest.PlugIn.Kernel"/>
        <property name="org.openmdx.jdo.DataManager.xriPattern[1]" value="xri://@openmdx*org.opencrx.kernel.activity1/($...)"/>
