<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE orion-application PUBLIC "-//ORACLE//DTD OC4J Application runtime 9.04//EN" "http://xmlns.oracle.com/ias/dtds/orion-application-9_04.dtd">
<orion-application deployment-version="10.1.2.0.0">
	<ejb-module remote="false" path="kernel.jar"/>
	<ejb-module remote="false" path="gateway.jar"/>
	<ejb-module remote="false" path="security.jar"/>
	<web-module id="opencrx-core-CRX-Web" path="opencrx-core-CRX-Web.war"/>
	<web-module id="client-gateway" path="client-gateway.war"/>
	<web-module id="server-gateway" path="server-gateway.war"/>
	<security-role-mapping name="OpenCrxUser">
		<group name="OpenCrxUser"/>
	</security-role-mapping>
	<security-role-mapping name="OpenCrxAdministrator">
		<group name="OpenCrxAdministrator"/>
	</security-role-mapping>
	<security-role-mapping name="OpenCrxRoot">
		<group name="OpenCrxRoot"/>
	</security-role-mapping>	
</orion-application>
