			<ejb-ref>
				<ejb-ref-name>ejb/dataprovider[4]</ejb-ref-name>
				<jndi-name>EXTERNAL_PROVIDER_JNDI_NAME_FORMAT1</jndi-name>
			</ejb-ref>		  
		  EXTERNAL_PROVIDER_ENABLE_END -->
			<resource-ref>
				<res-ref-name>jdbc/datasource[0]</res-ref-name>
				<jndi-name>jdbc/opencrx_CRX</jndi-name>
			</resource-ref>
			<pass-by-reference>false</pass-by-reference>
