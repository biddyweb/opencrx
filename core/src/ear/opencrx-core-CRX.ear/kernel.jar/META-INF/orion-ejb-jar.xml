<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE orion-ejb-jar PUBLIC "-//ORACLE//DTD OC4J Enterprise JavaBeans runtime 9.04//EN" "http://xmlns.oracle.com/ias/dtds/orion-ejb-jar-9_04.dtd">
<orion-ejb-jar>
	<enterprise-beans>
		<session-deployment name="opencrx_core_CRX_kernel_mandatory" max-tx-retries="0" location="org/opencrx/core/CRX/mandatory/kernel">
			<ejb-ref-mapping location="org/opencrx/core/CRX/mandatory/security" name="ejb/dataprovider[0]"/>
			<ejb-ref-mapping location="org/opencrx/core/CRX/mandatory/security" name="ejb/dataprovider[1]"/>
			<ejb-ref-mapping location="org/opencrx/core/CRX/mandatory/security" name="ejb/dataprovider[2]"/>
			<ejb-ref-mapping location="org/opencrx/core/CRX/mandatory/security" name="ejb/dataprovider[3]"/>
			<!-- EXTERNAL_PROVIDER_ENABLE_BEGIN
			  <ejb-ref-mapping location="EXTERNAL_PROVIDER_JNDI_NAME_FORMAT2" name="ejb/dataprovider[4]"/>
			EXTERNAL_PROVIDER_ENABLE_END -->
			<resource-ref-mapping name="jdbc/datasource[0]" location="jdbc/opencrx_core_CRX"/>
		</session-deployment>
	</enterprise-beans>
	<assembly-descriptor>
		<default-method-access>
			<security-role-mapping name="&lt;default-ejb-caller-role>" impliesAll="true"/>
		</default-method-access>
	</assembly-descriptor>
</orion-ejb-jar>
